#include<iostream>
#include<vector>
using namespace std;

int n;

int main() {
	cin >> n;
	vector<int> a(n);
	for (int i = 0; i < n; i++) {
		cin >> a[i];
	}
	int b, c;
	cin >> b >> c;
	long long ans = 0;
	for (int i = 0; i < n; i++) {
		ans += 1;
		if (a[i] > b) {
			int sum = a[i] - b;
			if (sum % c != 0) {
				ans += (sum / c) + 1;
			}
			else if (sum % c == 0) {
				ans += (sum / c);
			}
		}
	}
	cout << ans << '\n';
	return 0;
}
