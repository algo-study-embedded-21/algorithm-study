#include <iostream>
#include <vector>
using namespace std;

struct Node {
	int m;
	int s;
	int d;
};

vector<Node> MAP[51][51];

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie();
	cout.tie();

	// 입력
	int N, M, K;
	cin >> N >> M >> K;
	for (int i = 0; i < M; i++) {
		int r, c, m, s, d;
		cin >> r >> c >> m >> s >> d;
		MAP[r][c].push_back({ m,s,d });
	}

	// K번 반복
	for (int k = 0; k < K; k++) {
		// 모든 파이어볼 이동
		for (int i = 1; i <= N; i++) {
			for (int j = 1; j <= N; j++) {
				// 현재 칸에 파이어볼이 없다면 continue
				if (MAP[i][j].empty()) continue;
				
			}
		}

		// 이동하려는 위치에 질량 +, 속력 +
		// 이동하려는 위치의 방향
	}



	return 0;
}
